{
    "name": "s2hub/silverstripe-textassistant",
    "description": "Text assistant module, for generating translations and texts via LLM integrations",
    "type": "silverstripe-vendormodule",
    "homepage": "https://www.s2-hub.com/",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "textassistant"
    ],
    "authors": [
        {
            "name": "S2-Hub",
            "homepage": "https://www.s2-hub.com/"
        }
    ],
    "require": {
        "silverstripe/framework": "^5",
        "symbiote/silverstripe-queuedjobs": "^4.0|^5.0",
        "colymba/gridfield-bulk-editing-tools": "~3.0|~4.0"
    },
    "autoload": {
        "psr-4": {
            "S2Hub\\TextAssistant\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
